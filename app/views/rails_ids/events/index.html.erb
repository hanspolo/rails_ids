<h1><%= t('.events') %></h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>TYPE</th>
      <th>DATE</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.event_type %></td>
        <td><%= event.created_at %></td>
        <td><%= link_to '->', rails_ids.event_path(event) %>
      </tr>
    <% end %>
  </tbody>
</table>
